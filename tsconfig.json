{
  "extends": "@favware/ts-config",
    "compilerOptions": {
        "outDir": "build",
        "rootDir": "src"
    },
    "include": ["src"],
    "typedocOptions": {
      "out": "./docs/",
      "readme": "./README.md",
      "name": "YarnTs Template",
      "exclude": [
        "**/node_modules/**",
        "**/*.test.ts",
        "**/*.js"
      ],
      "mode": "modules",
      "excludeExternals": true,
      "excludePrivate": true,
      "excludeProtected": true,
      "excludeNotExported": true
    }
}